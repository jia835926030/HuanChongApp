    android:layout_height="2px"
    android:background="@color/colorBlack"/>
    <EditText
        android:id="@+id/mEditText"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:hint=""/>
</com.zhy.autolayout.AutoLinearLayout>
