<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60px">

        <ImageView
            android:id="@+id/set_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/back_gray" />

        <TextView
            android:layout_width="150px"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="设置"

            android:textSize="20sp" />

        <TextView
            android:id="@+id/set_send"
            android:layout_width="80px"
            android:layout_height="40px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:text="确定"
            android:visibility="gone" />
    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:background="#1bd6d4d3" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/proposal"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i1"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/proposal_img"
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@+id/i1"
            android:background="@drawable/publish_stories" />

        <TextView
            android:id="@+id/product_proposal_text"
            android:layout_width="200px"
            android:layout_height="55px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="产品建议"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="#d9dad8" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/introduce"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i2"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i2"
            android:background="@drawable/new_function" />

        <TextView
            android:layout_width="200px"
            android:layout_height="55px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="新功能介绍"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="#d9dad8" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/score"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i3"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i3"
            android:background="@drawable/praise" />

        <TextView
            android:layout_width="200px"
            android:layout_height="55px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="去评分"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="#d9dad8" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/about"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i4"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i4"
            android:background="@drawable/know" />

        <TextView
            android:layout_width="200px"
            android:layout_height="55px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="关于欢欢"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10px"
        android:background="#77bcbeba" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/wifi_display"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i5"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i5"
            android:background="@drawable/picture_wifi" />

        <TextView
            android:layout_width="270px"
            android:layout_height="60px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="仅在WIFI下显示图片"
            android:textStyle="bold" />

        <ToggleButton
            android:id="@+id/toggleButton"
            android:layout_width="120px"
            android:layout_height="50px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="#00000000"
            android:button="@drawable/bs_button_style"
            android:checked="false"
            android:gravity="center"
            android:textOff=""
            android:textOn="" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/cache"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i6"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/cache_img"
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i6"
            android:background="@drawable/delete_pet" />

        <TextView
            android:id="@+id/qw"
            android:layout_width="270px"
            android:layout_height="60px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="图片缓存 文件大小"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textViewhua"
            android:layout_width="120px"
            android:layout_height="55px"
            android:layout_alignLeft="@+id/toggleButton"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/toggleButton"
            android:layout_toLeftOf="@+id/zx"
            android:gravity="center"
            android:text="0KB" />

        <ImageView
            android:id="@+id/zx"
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/scavenging_cache"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:gravity="center">

        <View
            android:id="@+id/i7"
            android:layout_width="30px"
            android:layout_height="55px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="55px"
            android:layout_height="60px"
            android:layout_toRightOf="@id/i7"
            android:background="@drawable/cache" />

        <TextView
            android:layout_width="200px"
            android:layout_height="55px"
            android:layout_marginLeft="80px"
            android:gravity="center"
            android:text="清除缓存"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50px"
            android:layout_height="55px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/enter" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="20px"
        android:background="#05c9c8c6" />

    <Button
        android:id="@+id/sign_out"
        android:layout_width="690px"
        android:layout_height="90px"
        android:layout_gravity="center"
        android:background="#77dadad8"
        android:text="退出登录"
        android:textColor="#77f5051d" />
</com.zhy.autolayout.AutoLinearLayout>
